{
    "version": 2,
    "devCommand": "uvicorn app.main:start --reload",
    "builds": [
      {
        "src": "app/main.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    
    "routes": [
      { 
        "src": "/(.*)",
        "dest": "app/main.py"
      }
    ]

  }