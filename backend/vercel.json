{
    "version": 2,
    "builds": [
      {
        "src": "app/main.py",
        "use": "@vercel/python"
      }
    ],
    "devCommand": "uvicorn app.main:start --reload --host 0.0.0.0 --port 10000",
    "routes": [
      { 
        "src": "/(.*)",
        "dest": "app/main.py"
      }
    ],
    "env": {
      "APP_MODULE": "app.main:app"
    }
  }